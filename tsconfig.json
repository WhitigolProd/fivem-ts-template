{
    "compilerOptions": {
      "target": "ES2020",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "module": "CommonJS",
      "types": ["@citizenfx/client", "@citizenfx/server", "@types/node"],
      "lib": ["ES2020"],
      "skipLibCheck": true,
      "resolveJsonModule": true,
    },
    "include": ["./src/**/*"],
    "exclude": [
        "node_modules",
        "dist"
    ]
}